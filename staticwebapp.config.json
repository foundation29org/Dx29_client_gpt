{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, x-api-key, Accept, Accept-Language, Origin, User-Agent"
      },
      "rewrite": "https://dxgpt.app/api/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "403": {
      "rewrite": "/index.html",
      "statusCode": 200
    },
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
} 